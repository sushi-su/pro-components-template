{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "vis": "tsc && cross-env vis=1 vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "eslint": "eslint src/ --ext .ts,.tsx --fix",
    "stylelint": "stylelint 'src/**/*.less' 'src/**/*.css' --fix",
    "lint": "npm run eslint && npm run stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint",
      "git add"
    ],
    "*.{css, less}": [
      "npm run stylelint",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.3.6",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.18.42",
    "@ant-design/pro-descriptions": "^1.10.58",
    "@ant-design/pro-field": "^1.33.3",
    "@ant-design/pro-form": "^1.59.3",
    "@ant-design/pro-layout": "^7.0.1-beta.11",
    "@ant-design/pro-list": "^1.21.44",
    "@ant-design/pro-provider": "^1.6.1",
    "@ant-design/pro-skeleton": "^1.0.5",
    "@ant-design/pro-table": "^2.68.3",
    "@loadable/component": "^5.15.2",
    "ahooks": "^3.1.14",
    "antd": "^4.19.3",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "omit.js": "^2.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.16",
    "react-router-dom": "^6.2.2",
    "recoil": "^0.6.1",
    "store2": "^2.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/js-cookie": "^3.0.1",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.180",
    "@types/react": "^17.0.42",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.0",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-packagejson": "^2.2.17",
    "rc-field-form": "^1.24.0",
    "rollup": "^2.70.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^14.6.0",
