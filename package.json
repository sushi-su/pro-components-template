{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "vis": "tsc && cross-env vis=1 vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "eslint": "eslint src/ --ext .ts,.tsx --fix",
    "stylelint": "stylelint 'src/**/*.less' 'src/**/*.css' --fix",
    "lint": "npm run eslint && npm run stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint",
      "git add"
    ],
    "*.{css, less}": [
      "npm run stylelint",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.3.5",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.18.37",
    "@ant-design/pro-descriptions": "^1.10.52",
    "@ant-design/pro-field": "^1.32.4",
    "@ant-design/pro-form": "^1.57.0",
    "@ant-design/pro-layout": "^6.34.1",
    "@ant-design/pro-list": "^1.21.37",
    "@ant-design/pro-provider": "^1.6.0",
    "@ant-design/pro-skeleton": "^1.0.5",
    "@ant-design/pro-table": "^2.66.4",
    "@loadable/component": "^5.15.2",
    "ahooks": "^3.1.13",
    "antd": "^4.19.1",
    "axios": "^0.26.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.8",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "omit.js": "^2.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.16",
    "react-router-dom": "^6.2.2",
    "recoil": "^0.6.1",
    "store2": "^2.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/js-cookie": "^3.0.1",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.179",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-plugin-react": "^7.29.3",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.1",
    "rc-field-form": "^1.24.0",
    "rollup": "^2.70.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^14.5.3",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "webpack": "^5.70.0"
  }
