{
  "name": "pro-components-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rimraf dist stats.html node_modules pnpm-lock.yaml",
    "dev": "vite",
    "eslint": "eslint src/ --ext .ts,.tsx --fix",
    "lint": "npm run eslint && npm run stylelint",
    "prepare": "husky install",
    "preview": "vite preview",
    "stylelint": "stylelint 'src/**/*.less' 'src/**/*.css' --fix",
    "vis": "tsc && cross-env vis=1 vite build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint",
      "git add"
    ],
    "*.{css, less}": [
      "npm run stylelint",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.3.6",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.19.4",
    "@ant-design/pro-descriptions": "^1.10.66",
    "@ant-design/pro-field": "^1.34.4",
    "@ant-design/pro-form": "^1.64.0",
    "@ant-design/pro-layout": "^7.0.1-beta.13",
    "@ant-design/pro-list": "^1.21.53",
    "@ant-design/pro-provider": "^1.6.3",
    "@ant-design/pro-skeleton": "^1.0.5",
    "@ant-design/pro-table": "^2.71.6",
    "@loadable/component": "^5.15.2",
    "ahooks": "^3.3.8",
    "antd": "^4.20.0-alpha.0",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.1",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "omit.js": "^2.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^4.0.0-beta.3",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.2",
    "store2": "^2.13.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/js-cookie": "^3.0.1",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.181",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-react": "^1.3.1",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.8",
    "postcss": "^8.4.12",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-packagejson": "^2.2.17",
    "rollup": "^2.70.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^14.7.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  }
}
